{
  "name": "shortify",
  "version": "1.0.0",
  "description": "short url",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && npm run copy-assets",
    "start": "node dist/index.js",
    "dev": "concurrently \"ts-node-dev src/index.ts\" \"cd client && npm start\"",
    "test": "jest",
    "copy-assets": "cp -r src/ dist/ && cp .env dist/",
    "client": "cd client && npm start",
    "server": "ts-node-dev src/index.ts"
  },
  "dependencies": {
    "express": "^4.18.2",
    "redis": "^4.6.10",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/redis": "^4.0.11",
    "@types/cors": "^2.8.13",
    "ts-node-dev": "^2.0.0",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ameagol/tinylink.git"
  },
  "keywords": [
    "short",
    "url"
  ],
  "author": "Ricardo Gellman",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ameagol/tinylink/issues"
  },
  "homepage": "https://github.com/ameagol/tinylink#readme"
}